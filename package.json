{
  "name": "strapi-connector-firestore",
  "version": "3.0.0-alpha.8",
  "description": "Strapi database connector for Firestore database on Google Cloud Platform.",
  "keywords": [
    "firestore",
    "hook",
    "orm",
    "nosql",
    "strapi"
  ],
  "author": "Arrowhead Apps Ltd",
  "license": "MIT",
  "repository": "github:arrowheadapps/strapi-connector-firestore",
  "main": "lib/index.js",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "files": [
    "lib/**"
  ],
  "scripts": {
    "build": "rm -rf lib && tsc",
    "prepare": "tsc --skipLibCheck",
    "test": "tsc --skipLibCheck && node test/e2e.js",
    "test:e2e": "jest --config jest.config.e2e.js --runInBand --verbose --forceExit --detectOpenHandles"
  },
  "dependencies": {
    "@google-cloud/firestore": "^4.0.0",
    "lodash": "^4.17.15",
    "strapi-utils": "3.0.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.14",
    "typescript": "^3.9.5"
  }
}
